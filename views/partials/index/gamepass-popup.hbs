<section class='gamepass-popup popup-instruction'>
	<div class='popup-instruction__container'>
		<div class='popup-instruction__content'>
			<div class='block' id='block_wrap'>
				<div class='block__wrapper' id='block_wrapper1'>

					<div class='block_content'>
